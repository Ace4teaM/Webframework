   /** @brief Convert name to code */
    public static function nameToCode($name){
        for($i=strlen($name)-1;$i&gt;=0;$i--){
            $c = substr($name, $i, 1);
            if(strpos(&quot;ABCDEFGHIJKLMNOPQRSTUVWXYZ&quot;,$c) !== FALSE){
                $name = substr_replace($name,($i?&quot;_&quot;:&quot;&quot;).strtolower($c), $i, 1);
            }
        }
        return $name;
    }
    
