       global $app;
       if(!$db &amp;&amp; !$app-&gt;getDB($db))
         return false;
      
      //execute la requete
       $query = &quot;SELECT * from %Name% where $cond&quot;;
       if($db-&gt;execute($query,$result)){
            $inst = new %Code%();
