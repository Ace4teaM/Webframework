        $node-&gt;appendChild($doc-&gt;createTextElement(&quot;
.convert_name(%Name%,&quot;_&quot;,&quot; &quot;)
&quot;,$inst-&gt;%Code%));
       .next(&quot;\n&quot;)
       \n
          
        return $node;
    }
    
    
    /*
